ontainer-fluid {\n  width: 100%;\n  max-width: 1400px;\n}\n.btn {\n  border: 1px solid #adadad;;\n}\n.navbar {\n  border-radius: 0;\n}\n.jumbotron {\n  background: #f7f7f7;\n}\n#work-details.hide {\n  opacity: 0 !important;\n}\n#work-details.fade {\n  opacity: 1 !important;\n  margin-bottom: 5em;\n  transition: 1s ease-in-out;\n}\nsection#work-gallery button:focus {\n  background: rgb(192, 192, 192);\n  outline: 0;\n}\n.project-cover-art > img {\n  border-top-right-radius: 2px;\n  border-top-left-radius: 2px;\n}\n/***********************/\n/*       desktop       */\n/***********************/\nsection#work-gallery {\n  padding: 5em 0em 5em 0em;\n  & > div {\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    grid-column-gap: 1rem;\n  }\n  .overlay {\n    position: absolute;\n    top: 0;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n    padding: 2em;\n    height: 10px;\n    width: 100%;\n    background: rgba(0, 0, 0, 0.5);\n    color: white;\n    opacity: 0;\n    text-align: center;\n  }\n  .project-cover-art:hover .overlay {\n    opacity: 1;\n    transition: 0.5s ease-in;\n    filter: blur(0px);\n  }\n}\nsection#footer {\n  background: #f7f7f7;\n  margin-top: 0em;\n  footer{\n    border-top: 1px solid #e7e7e7;\n    padding: 0.3em 0em 1em 0em;\n    .footer__inner {\n      padding: 3rem 0;\n      display: flex;\n      justify-content: space-evenly;\n      align-items: baseline;\n    }\n  }\n  input {\n    padding: 0.5em 0em;\n    border-top-style: none;\n    border-right-style: none;\n    border-bottom-style: solid;\n    border-left-style: none;\n    outline: none;\n    background: transparent;\n    border-radius: 0;\n  }\n  textarea {\n    resize: none;\n    border: 2px solid black;\n    border-top-style: none;\n    border-right-style: none;\n    border-bottom-style: solid;\n    border-left-style: none;\n    outline: none;\n    padding: 0.5em 0em;\n    background: transparent;\n    border-radius: 0;\n  }\n  button[type=\"submit\"] {\n    background: transparent;\n    border: none;\n    padding: 5px 0 5px 0;\n    border-bottom: 3px solid red;\n    &:hover {font-weight: bold}\n  }\n  input[type=\"submit\"]:hover {\n    font-weight: 900;\n  }\n  .footer-column p:first-child{\n    padding: 2rem 0rem;\n  }\n  .footer-column {\n    a {\n      color: black;\n      text-decoration: underline;\n    }\n  }\n}\n\nsection#navigation {\n  .navbar-header {\n    img {\n      max-width: 150px\n    }\n  }\n  .navbar-social-media {\n    padding: 0;\n    font-size: 1.2em;\n    font-family: Sarala;\n    padding: 1em 0em 0em 1em;\n    span {\n      padding: 0 1em 0em 0em;\n    }\n  }\n  a {\n    color: black;\n  }\n  .navbar-default {\n    background: rgba(255, 255, 255, 0.9)\n  }\n  .navbar-social-media {\n    font-size: 1.3em;\n    padding: 1em 0em;\n    & > a {\n      font-weight: bold;\n    }\n    i {\n      padding: 0em 0.5em;\n    }\n  }\n  .navbar-header {\n    font-size: 1em;\n    padding: 0.8em 0em 0em 1em;\n    a {\n      color: black;\n    }\n  }\n  .navigation-style {\n    width: 80%;\n    margin: 0px auto;\n    .navigation-style__inner {\n      display: flex;\n      justify-content: space-between;\n      flex-direction: row;\n    }\n  }\n}\n\nsection#banner {\n  button {\n    margin: 0em 0.5em;\n  }\n}\n\nsection#work-details {\n  padding: 5em 0em 0em 0em;\n  .project-description {\n    display: block;\n    padding: 0.2em 3em;\n    font-size: 1.1em;\n    font-weight: bolder;\n  }\n  .project-header {\n    display: block;\n    padding: 0em 1.3em;\n    font-size: 1.1em;\n    font-weight: bolder;\n  }\n  a {\n    text-decoration: none;\n    color: black;\n  }\n  p {\n    position: relative;\n    font-size: 1.1em;\n    padding: 1em 0em\n  }\n  .project-description:before {\n    content: \"\";\n    background: black;\n    position: absolute;\n    top: 29px;\n    left: 0;\n    width: 37px;\n    height: 4px;\n  }\n  .tag {\n    display: inline-block;\n    padding: 0.1em 1em;\n    margin: 0.2em 0.2em;\n    background: #e8e8e8;\n    border-radius: 2px;\n  }\n}\n\n/***********************/\n/*       mobile        */\n/***********************/\n\n@media only screen and(max-width: 993px){\n  section#footer {\n    text-align: center;\n  }\n  section#navigation {\n    .navbar-header {\n      padding: 0.5em 0em 0em 1em;\n    }\n    .navbar-social-media {\n      padding: 0;\n      font-size: 1em;\n      padding: 1.2em 0em 0em 1em;\n      font-weight: bolder;\n      span {\n        padding: 0 0.3em;\n      }\n    }\n    .navigation-style {\n      width: 85%;\n      margin: 0px auto;\n    }\n  }\n  section#work-details {\n    .space {\n      margin: 2.5em 0;\n    }\n  }\n  section#work-gallery {\n    & > div {\n      grid-template-columns: repeat(2, 1fr);\n    }\n  }\n}\n\n@media only screen and(max-width: 768px){\n  section#navigation {\n    .navigation-style {\n      width: 100%;\n      margin: 0px auto;\n    }\n  }\n  section#work-gallery {\n    & > div {\n      grid-template-columns: 1fr;\n    }\n  }\n}\n\n@media only screen and(max-width: 500px){\n  .banner__text {\n    h1 {\n      font-size: 2.5rem;\n    }\n    h4 {\n      font-size: 1.5rem;\n    }\n  }\n  .navigation-style {\n    .navigation-style__inner {\n      flex-direction: column !important;\n      align-items: center;\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"506c09c9dbba256f477a464bb6551a9b","cacheData":{"env":{}}}